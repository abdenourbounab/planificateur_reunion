# Fichier .env pour la configuration
# Copier ce fichier en .env et remplir avec vos vraies valeurs

# Base de données
DATABASE_URL=mysql+pymysql://root:password@localhost/meeting_planner

# Groq API
GROQ_API_KEY=

# Configuration des modèles LLM (modèles disponibles sur Groq)
ORCHESTRATOR_MODEL=
INVITATION_MODEL=

# Température des modèles (0.0 = déterministe, 1.0 = créatif)
ORCHESTRATOR_TEMPERATURE=
INVITATION_TEMPERATURE=

# Mode debug
DEBUG=True
